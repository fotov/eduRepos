		<field xsi:type="DataSetFieldField">
			<dataPath>ДокументТехПроект</dataPath>
			<field>ДокументТехПроект</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Документ тех проект</v8:content>
				</v8:item>
			</title>
